{
  "name": "parcel-transformer-rough-svg",
  "version": "0.1.0",
  "description": "A Parcel transformer to rough up SVGs using Rough.js.",
  "license": "MIT",
  "repository": "github:thewilkybarkid/parcel-transformer-rough-svg",
  "keywords": [
    "parcel",
    "transformer",
    "svg",
    "roughjs",
    "hand-drawn",
    "sketch"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=14",
    "parcel": "^2.0.0-rc.0"
  },
  "dependencies": {
    "@parcel/plugin": "^2.0.0-rc.0",
    "rougher": "^1.1.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@prettier/plugin-xml": "^1.0.2",
    "bats": "^1.4.1",
    "bats-assert": "^2.0.0",
    "bats-support": "^0.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.1",
    "prettier": "^2.3.2",
    "prettier-plugin-sh": "^0.7.1",
    "standard-version": "^9.3.1"
  },
  "scripts": {
    "format": "prettier --ignore-unknown --check '**'",
    "format:fix": "npm run format -- --write",
    "lint": "eslint .",
